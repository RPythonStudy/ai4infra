install-bitwarden:
	$(shell which python) scripts/ai4infra/ai4infra-cli.py install bitwarden

install-bitwarden--reset:
	$(shell which python) scripts/ai4infra/ai4infra-cli.py install bitwarden --reset

backup-bitwarden:
	$(shell which python) scripts/ai4infra/ai4infra-cli.py backup bitwarden

restore-bitwarden:
	$(shell which python) scripts/ai4infra/ai4infra-cli.py restore bitwarden

install-postgres:
	$(shell which python) scripts/ai4infra/ai4infra-cli.py install postgres

install-postgres--reset:
	$(shell which python) scripts/ai4infra/ai4infra-cli.py install postgres --reset

install-vault:
	$(shell which python) scripts/ai4infra/ai4infra-cli.py install vault

install-vault--reset:
	$(shell which python) scripts/ai4infra/ai4infra-cli.py install vault --reset

init-vault:
	$(shell which python) scripts/ai4infra/ai4infra-cli.py init-vault

unseal-vault:
	$(shell which python) scripts/ai4infra/ai4infra-cli.py unseal-vault

backup-vault:
	$(shell which python) scripts/ai4infra/ai4infra-cli.py backup vault

restore-vault:
	$(shell which python) scripts/ai4infra/ai4infra-cli.py restore vault

install-all:
	$(shell which python) scripts/ai4infra/ai4infra-cli.py install all